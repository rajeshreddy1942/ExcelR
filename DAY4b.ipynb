   "source": [
    "def sum_of_even_numbers(n):\n",
    "    sum_even = 0\n",
    "    for i in range(2, n + 1, 2):\n",
    "        sum_even += i\n",
    "    return sum_even\n",
    "\n",
    "n = int(input(\"Enter a positive integer: \"))\n",
    "\n",
    "sum_even = sum_of_even_numbers(n)\n",
    "print(\"Sum of all even numbers between 1 and\", n, \"is:\", sum_even)\n"
   ]
